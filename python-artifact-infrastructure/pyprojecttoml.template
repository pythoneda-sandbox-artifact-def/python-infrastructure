[tool.poetry]
name = "@pname@"
version = "@version@"
description = "@desc@"
authors = [ @authors@ ]
readme = "README.md"
homepage = "@homepage@"
packages = [{include = "@package@"}]

[tool.poetry.dependencies]
python = "^@pythonMajorMinorVersion@"
pythoneda-sandbox-python-artifact = "^@pythonedaSandboxPythonArtifact@"
pythoneda-shared-artifact-changes-events-infrastructure = "^@pythonedaSharedArtifactChangesEventsInfrastructure@"
pythoneda-shared-pythoneda-domain = "^@pythonedaSharedPythonedaDomain@"
pythoneda-shared-pythoneda-infrastructure = "^@pythonedaSharedPythonedaInfrastructure@"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
